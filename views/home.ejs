<% include partials/header %>

    <h1>Home</h1>
    <p>
        <%= homeStartingContent %>
    </p>
    <% posts.forEach(element=> { %>
        <div>
            <h1>
                <%= element.title %>
            </h1>
            <p>
                <%= element.content.substr(0, 100) %> ... <a href="/posts/<%=element.title %>">Read More</a>
            </p>
            <% }) %>
        </div>
        <% include partials/footer %>